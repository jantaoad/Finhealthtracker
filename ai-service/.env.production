# AI Service Environment Configuration
# यह file App Runner में environment variables के लिए reference है

FLASK_ENV=production
PORT=5000

# Backend Integration
BACKEND_URL=https://finhealthtracker-backend-XXXXX.awsapprunner.com
BACKEND_API_KEY=your-api-key-change-this

# AI Model Configuration
MODEL_PATH=/app/models
MODEL_TYPE=production
CACHE_ENABLED=true

# Logging
LOG_LEVEL=INFO
DEBUG=false

# Performance
MAX_WORKERS=4
TIMEOUT=300
